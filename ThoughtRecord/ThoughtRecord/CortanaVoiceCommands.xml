<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">

  <CommandSet xml:lang="en-ca" Name="LaunchCommandSet_en-ca">
    <AppName>Thought Record</AppName>
    <Example> Open my latest entry </Example>

    <Command Name="OpenLatestRecord">
      <Example> Open my latest entry </Example>
      <ListenFor> Open [my] latest {builtin:AppName} </ListenFor>
      <ListenFor> Open [my] last {builtin:AppName} </ListenFor>
      <ListenFor> Open [my] newest {builtin:AppName} </ListenFor>
      <ListenFor> Show [me] [my] newest {builtin:AppName} </ListenFor>
      <ListenFor> Show [me] [my] latest {builtin:AppName} </ListenFor>
      <Feedback> Opening your latest thought record... </Feedback>
      <Navigate/>
    </Command>
    
  <Command Name="OpenNewRecord">
      <Example> Open new </Example>
      <ListenFor> Open [a] new {builtin:AppName} </ListenFor>
      <ListenFor> Open [a] blank {builtin:AppName} </ListenFor>
      <Feedback> Opening a new thought record... </Feedback>
      <Navigate />
    </Command>
    
  <Command Name="OpenRecordList">
      <Example> Show me all </Example>
      <ListenFor> Show [me] all {builtin:AppName}s </ListenFor>
      <ListenFor> Show [me] [my] {builtin:AppName}s </ListenFor>
      <ListenFor> List [me] [my] {builtin:AppName}s </ListenFor>
      <Feedback> Opening your thought records... </Feedback>
      <Navigate />
    </Command>
    
    <Command Name="OpenHelp">
      <Example> Open help </Example>
      <ListenFor> Open [the] {builtin:AppName} help [page] </ListenFor>
      <ListenFor> How do I use [a] {builtin:AppName}</ListenFor>
      <ListenFor> How do you use [a] {builtin:AppName}</ListenFor>
      <ListenFor> What is [a] {builtin:AppName} </ListenFor>
      <ListenFor> What Cortana commands work with {builtin:AppName} </ListenFor>
      <Feedback> Opening Thought Record help page... </Feedback>
      <Navigate/>
    </Command>

    <Command Name="SaveThoughtRecord">
      <Example> Save thought record </Example>
      <ListenFor> Save [my] [current] {builtin:AppName}</ListenFor>
      <ListenFor> Save this {builtin:AppName}</ListenFor>
      <Feedback> Saving your thought record... </Feedback>
      <Navigate />
    </Command>
    
  <Command Name="DeleteThoughtRecord">
      <Example> Delete thought record </Example>
      <ListenFor> Delete [this] [current] {builtin:AppName}</ListenFor>
      <Feedback> Deleting your current thought record... </Feedback>
      <Navigate />
    </Command>
    <Command Name="EditThoughtRecord">
      <Example> Edit thought record </Example>
      <ListenFor> Edit [this] [current] {builtin:AppName}</ListenFor>
      <Feedback> Preparing your current thought record for editing... </Feedback>
      <Navigate />
    </Command>

  </CommandSet>
  
